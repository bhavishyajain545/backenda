{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/users/(.*)",
            "dest": "/api/users/index.js"
        },
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth/index.js"
        },
        {
            "src": "/api/posts/(.*)",
            "dest": "/api/posts/index.js"
        }
    ]
}